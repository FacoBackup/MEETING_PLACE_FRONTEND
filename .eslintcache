   this.setState({\n                community: res.data\n            })\n        })\n        .catch(error=>console.log(error))\n    }\n    \n\n    renderOptions(){\n        switch(true){\n            case this.state.related: {\n                return(\n                    <div>\n                        <CommunityRelatedComponent communityID={this.state.communityID} />\n                    </div>\n                )   \n            }\n        \n            case (this.state.membersOption):{\n                console.log(\"MEMBERS\")\n                return(\n                    <div>\n                        <CommunityUsersComponent token={this.state.token} role={this.state.community.role} communityID={this.state.communityID} options={1}/>\n                    </div>\n                )\n            }\n            case this.state.all:{\n                console.log(\"ALL\")\n                return(\n                    <div>\n                        <CommunityUsersComponent token={this.state.token} role={this.state.community.role} communityID={this.state.communityID}/>\n                    </div>\n                ) \n            }\n            case this.state.mods:{\n                console.log(\"MODS\")\n                return(\n                    <div>\n                        <CommunityUsersComponent token={this.state.token} communityID={this.state.communityID} options={2}/>\n                    </div>\n                )\n            }\n            case this.state.followers:{\n                console.log(\"FOLLOWERS\")\n                return(\n                    <div>\n                        <CommunityUsersComponent token={this.state.token} communityID={this.state.communityID} options={0}/>\n                    </div>\n                )\n            }\n            case this.state.topic:{\n                return(\n                    <div >\n                        <TopicComponent community={true} token={this.state.token} timeline={false} subjectID={this.state.communityID}/>\n                    </div>\n                )\n            }\n            case this.state.about:{\n                return(\n                    <div>\n                        <CommunityAboutComponent community={this.state.community} />\n                    </div>\n                )\n            }\n            default:{\n                return(\n                    <TopicComponent community={true} token={this.state.token} timeline={false} subjectID={this.state.communityID}/>\n                )\n            }\n        }\n    }\n    \n\n    render(){\n        if(typeof this.state.community.name !== 'undefined'){\n            return(\n                <div className=\"dedicated_component_container\">\n                    <div className=\"dedicated_image_container\">\n                        <img className='dedicated_image_style' alt=\"BACKGROUD\" src= {(this.state.community.backgroundImageURL !== null && typeof this.state.community.backgroundImageURL !== 'undefined') ?  this.state.community.backgroundImageURL : \"https://www.beautycolorcode.com/2f2f2f-1440x900.png\"} />\n                        \n                    </div>\n                    <div className=\"dedicated_action_bar\">\n                        <Persona\n                            {...{\n                                imageUrl: this.state.community.imageURL,\n                                text: this.state.community.name,\n                                secondaryText: this.state.community.about,\n                                tertiaryText: \"Your're a \" + this.state.community.role + \" here\"\n                            }}\n                            size={PersonaSize.size72}\n                            imageAlt=\"Community\"\n                        />\n                        <div className=\"dedicated_action_bar_buttons\">\n\n                            {this.state.membersOption === true ? <PrimaryButton text='Members'style={{fontSize: '16px'}}/> : <DefaultButton style={{fontSize: '16px'}} text='Members' onClick={()=>\n                                 this.setState({\n                                    membersOption: true,\n                                    topic:false,\n                                    related: false,\n                                    about: false,\n                                    all: false,\n                                    followers:false,\n                                    mods:false,\n                                })\n                            }/> }\n\n                            {this.state.followers === true ? <PrimaryButton text='Followers'style={{fontSize: '16px'}}/> : <DefaultButton style={{fontSize: '16px'}} text='Followers' onClick={()=>\n                                 this.setState({\n                                    membersOption: false,\n                                    topic:false,\n                                    related: false,\n                                    about: false,\n                                    all: false,\n                                    followers:true,\n                                    mods:false,\n                                })\n                            }/> }\n                            {this.state.mods === true ? <PrimaryButton text='Moderators'style={{fontSize: '16px'}}/> : <DefaultButton text='Moderators'style={{fontSize: '16px'}} onClick={()=>\n                                    this.setState({\n                                        membersOption: false,\n                                        topic:false,\n                                        related: false,\n                                        about: false,\n                                        all: false,\n                                        followers:false,\n                                        mods:true,\n                                    })\n                            }/> }\n                            {this.state.all === true ? <PrimaryButton text='All Users'style={{fontSize: '16px'}}/> : <DefaultButton text='All Users'style={{fontSize: '16px'}} onClick={()=>\n                                  this.setState({\n                                    membersOption: false,\n                                    topic:false,\n                                    related: false,\n                                    about: false,\n                                    all: true,\n                                    followers:false,\n                                    mods:false,\n                                })\n                            }/> }\n                            {this.state.related === true ? <PrimaryButton text='Related Communities' style={{fontSize: '16px'}}/> : <DefaultButton  style={{fontSize: '16px'}} text='Related Communities' onClick={()=>\n                                this.setState({\n                                    membersOption: false,\n                                    topic:false,\n                                    related: true,\n                                    about: false,\n                                    all: false,\n                                    followers:false,\n                                    mods:false,\n                                })  \n                            }/> }\n                            {this.state.topic === true?  <PrimaryButton style={{fontSize: '16px'}} text='Topics'/> : <DefaultButton style={{fontSize: '16px'}} text='Topics' onClick={()=>\n                                this.setState({\n                                    membersOption: false,\n                                    topic:true,\n                                    related: false,\n                                    about: false,\n                                    all: false,\n                                    followers:false,\n                                    mods:false,\n                                })    \n                            }/>                              